{
  "name": "taskmill",
  "displayName": "TaskMill",
  "description": "Run Node Scripts hosted on GitHub through your editor",
  "version": "0.0.1",
  "publisher": "a7medkamel",
  "engines": {
    "vscode": "^0.10.6"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.run_git"
  ],
  "main": "./extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.run_git",
        "title": "TaskMill"
      }
    ],
    "keybindings": [
      {
        "command": "extension.run_git",
        "key": "Ctrl+Shift+T",
        "mac": "Cmd+Shift+T"
      }
    ]
  },
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "vscode": "^0.11.0"
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "content-type": "^1.0.1",
    "fs-extra": "^0.26.5",
    "lodash": "^4.5.1",
    "mime-db": "^1.22.0",
    "mime-type": "^3.0.4",
    "open": "0.0.5",
    "request": "^2.69.0",
    "request-promise": "^2.0.1",
    "tmp": "0.0.28"
  }
}
